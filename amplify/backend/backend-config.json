{
    "api": {
        "schmoffee": {
            "service": "AppSync",
            "providerPlugin": "awscloudformation",
            "dependsOn": [],
            "output": {
                "authConfig": {
                    "defaultAuthentication": {
                        "authenticationType": "API_KEY",
                        "apiKeyConfig": {
                            "apiKeyExpirationDays": 30,
                            "description": "api key description"
                        }
                    },
                    "additionalAuthenticationProviders": [
                        {
                            "authenticationType": "AWS_IAM"
                        }
                    ]
                }
            }
        }
    },
    "auth": {
        "schmoffeeAuth": {
            "service": "Cognito",
            "providerPlugin": "awscloudformation",
            "dependsOn": [
                {
                    "category": "function",
                    "resourceName": "schmoffeeAuthCreateAuthChallenge",
                    "triggerProvider": "Cognito",
                    "attributes": [
                        "Arn",
                        "Name"
                    ]
                },
                {
                    "category": "function",
                    "resourceName": "schmoffeeAuthDefineAuthChallenge",
                    "triggerProvider": "Cognito",
                    "attributes": [
                        "Arn",
                        "Name"
                    ]
                },
                {
                    "category": "function",
                    "resourceName": "schmoffeeAuthPreSignup",
                    "triggerProvider": "Cognito",
                    "attributes": [
                        "Arn",
                        "Name"
                    ]
                },
                {
                    "category": "function",
                    "resourceName": "schmoffeeAuthVerifyAuthChallengeResponse",
                    "triggerProvider": "Cognito",
                    "attributes": [
                        "Arn",
                        "Name"
                    ]
                }
            ],
            "customAuth": true,
            "frontendAuthConfig": {
                "socialProviders": [],
                "usernameAttributes": [],
                "signupAttributes": [
                    "NAME",
                    "PHONE_NUMBER"
                ],
                "passwordProtectionSettings": {
                    "passwordPolicyMinLength": 8,
                    "passwordPolicyCharacters": []
                },
                "mfaConfiguration": "OFF",
                "mfaTypes": [
                    "SMS"
                ],
                "verificationMechanisms": [
                    "PHONE_NUMBER"
                ]
            }
        }
    },
    "analytics": {
        "schmoffee": {
            "service": "Pinpoint",
            "providerPlugin": "awscloudformation"
        }
    },
    "function": {
        "schmoffeeAuthCreateAuthChallenge": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda"
        },
        "schmoffeeAuthDefineAuthChallenge": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda"
        },
        "schmoffeeAuthPreSignup": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda"
        },
        "schmoffeeAuthVerifyAuthChallengeResponse": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda"
        },
        "schmoffeePayWithStripe": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda"
        },
        "schmoffeeSendNotification": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda",
            "dependsOn": []
        },
        "schmoffeeCancelPayment": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda"
        }
    },
    "storage": {
        "storage": {
            "service": "S3",
            "providerPlugin": "awscloudformation",
            "dependsOn": []
        }
    },
    "notifications": {
        "schmoffee": {
            "service": "Pinpoint",
            "channels": [
                "FCM"
            ]
        }
    }
}